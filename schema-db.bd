class RepairTransaction:
    client = models.ForeignKey(ClientName)
    gadget_detail = models.ForeignKey(Gadget_detail)
    brought_in_date = models.DateTimeField()
    assigned_technician = models.ForeignKey(User)
    payment_status = models.CharField(choices=PAYMENT_STATUS)
    transaction_id = models.CharField(unique=True)





